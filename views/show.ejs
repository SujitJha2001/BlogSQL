<%- include('partials/header.ejs') %>

<% result.forEach((result) => { %>
    <section id="show">
        <div class="inside">
            <div class="image">
                <img src=<%=result.img_url%> alt="">
            </div>
            <div class="contents">
                <div class="title"><%=result.title%></div>
                <p class="description">
                    <%=result.description%>
                </p>
                <div class="show-btn">
                    <a class="delete-btn"href="/delete/<%=result.id%>">Delete</a>
                    <a class="edit-btn" href="/edit/<%=result.id%>">Edit</a>
                </div>
             
            </div>
        </div>
    </section>
    <%}) %>




<%- include('partials/footer.ejs') %>